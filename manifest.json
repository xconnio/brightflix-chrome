{
  "name": "Brightflix",
  "version": "1.0",
  "description": "Increases laptop's brightness when netflix.com goes fullscreen",
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "background.service_worker": [
      "js/autobahn.min.js",
      "js/background.js"
    ],
  "icons": {
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },

  "manifest_version": 3
}
